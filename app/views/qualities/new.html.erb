<%= form_for @qualities do |f| %>
	<%= f.label :ourbrother_id %>
	<%= f.text_field :ourbrother_id %>
	<br>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :image %>
	<%= f.text_field :image %>
	<br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<br>
	<%= f.submit %> 
	<%= link_to "back", :back %>
<% end %>